{
  "name": "emma-holland-design",
  "version": "1.0.1",
  "private": true,
  "author": "Kim Holland <kim@kimmr.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/tripleaxis/emmahollanddesign.git"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest",
    "lint": "eslint --fix \"{src,test}/**/*.js\"",
    "build": "webpack --env.production",
    "deploy": "firebase deploy",
    "precommit": "npm run lint",
    "commitmsg": "conventional-changelog-lint -e",
    "corp-release": "corp-semantic-release -b master"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/cz-config.js"
    }
  },
  "jest": {
    "clearMocks": true,
    "resetModules": true,
    "collectCoverage": true,
    "coverageDirectory": ".coverage",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/mocks/styleMock.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    }
  },
  "nyc": {
    "exclude": [
      "test/**/*.js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "report-dir": "reports"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "conventional-changelog-angular": "^1.3.3",
    "conventional-changelog-lint": "^1.1.9",
    "conventional-changelog-lint-config-angular": "^0.4.1",
    "corp-semantic-release": "^6.0.0",
    "css-loader": "^0.26.1",
    "cz-customizable": "^5.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.14.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.1",
    "firebase": "^3.4.1",
    "husky": "^0.13.4",
    "jasmine": "^2.5.3",
    "jasmine-spec-reporter": "^3.2.0",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.3",
    "moment": "^2.17.1",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.2.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.6.0",
    "react-datepicker": "^0.39.0",
    "react-dom": "^15.5.4",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "signals": "^1.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "whatwg-fetch": "^2.0.1"
  }
}
